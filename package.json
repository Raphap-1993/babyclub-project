{
  "name": "babyclub-monorepo",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:backoffice": "cd apps/backoffice && pnpm dev",
    "dev:landing": "cd apps/landing && pnpm dev",
    "lint": "turbo run lint",
    "test": "vitest run",
    "test:e2e": "playwright test --config=tests/e2e/playwright.config.ts",
    "test:e2e:ui": "playwright test --config=tests/e2e/playwright.config.ts --ui",
    "test:e2e:headed": "playwright test --config=tests/e2e/playwright.config.ts --headed",
    "test:e2e:debug": "playwright test --config=tests/e2e/playwright.config.ts --debug",
    "test:e2e:report": "playwright show-report tests/e2e/playwright-report",
    "test:e2e:codegen": "playwright codegen localhost:3001",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "@playwright/test": "^1.48.0",
    "@types/luxon": "^3.7.1",
    "@types/node": "^22.15.3",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "autoprefixer": "^10.4.22",
    "next": "16.0.7",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "3.4",
    "turbo": "^2.6.1",
    "typescript": "5.9.2",
    "vitest": "^2.1.4"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@supabase/supabase-js": "^2.86.0",
    "@types/react-datepicker": "^7.0.0",
    "@zxing/browser": "^0.1.4",
    "luxon": "^3.7.2",
    "react-datepicker": "^9.1.0",
    "recharts": "^3.7.0",
    "resend": "^6.6.0"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
